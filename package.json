{
  "name": "example-html-css-js-boilerplate",
  "version": "0.0.1",
  "description": "My boilerplate for showing html css and other front web tech",
  "author": "chenxuewei",
  "main": "index.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "start": "gulp",
    "test": "mocha ./src/js/test/test.js",
    "server": "nodemon --exec babel-node --presets env ./src/server.js"
  },
  "paths": {
    "src": {
      "html": "./src/html/**/*.html",
      "baseExampleHtml": "./src/html/basic-example/",
      "css": "./src/css/",
      "scss": "./src/scss/",
      "js": "./src/js/",
      "img": "./src/img/",
      "fonts": "./src/fonts/"
    },
    "dest": {
      "html": "./dist/html/",
      "baseExampleHtml": "./dist/html/basic-example/",
      "css": "./dist/css/",
      "scss": "./dist/scss/",
      "js": "./dist/js/",
      "img": "./dist/img/",
      "fonts": "./dist/fonts/"
    },
    "build": {},
    "bootstrap": {
      "src": [
        "./node_modules/bootstrap/dist/**/*",
        "!./node_modules/bootstrap/dist/css/bootstrap-grid*",
        "!./node_modules/bootstrap/dist/css/bootstrap-reboot*"
      ],
      "dest": "./vendor/bootstrap"
    }
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-sync": "2.23.6",
    "del": "^3.0.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "3.9.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-size": "^3.0.0",
    "gulp-useref": "^3.1.5",
    "mocha": "^5.2.0",
    "nodemon": "1.18.4",
    "run-sequence": "^2.2.1"
  }
}
